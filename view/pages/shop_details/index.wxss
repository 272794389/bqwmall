.activity {
  height: 40rpx;
  padding-left: 20rpx;
  border: 1rpx solid #f2857b;
  color: #e93323;
  font-size: 24rpx;
  line-height: 40rpx;
  position: relative;
  margin: 0 15rpx 1rpx 0;
  width: 40%;
  float: left;
  padding-left: 20rpx;

}
.activity:before {
  content: " ";
  position: absolute;
  width: 7rpx;
  height: 10rpx;
  border-radius: 0 7rpx 7rpx 0;
  border: 1px solid #f2857b;
  background-color: #fff;
  bottom: 50%;
  left: -2rpx;
  margin-bottom: -7rpx;
  border-left-color: #fff;
}
.activity:after {
  content: " ";
  position: absolute;
  width: 7rpx;
  height: 10rpx;
  border-radius: 7rpx 0 0 7rpx;
  border: 1px solid #f2857b;
  background-color: #fff;
  right: -2rpx;
  bottom: 50%;
  margin-bottom: -5rpx;
  border-right-color: #fff;
}
